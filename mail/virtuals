#For the DOMAIN-NAME substitute for yours e.g. example.com
#For <emailaccount> replace with your desired email account
#e.g. <emailaccount>@DOMAIN-NAME will become john@example.com

hostmaster@DOMAIN-NAME: <emailaccount>@DOMAIN-NAME
postmaster@DOMAIN-NAME: <emailaccount>@DOMAIN-NAME
webmaster@DOMAIN-NAME: <emailaccount>@DOMAIN-NAME
<emailaccount>@DOMAIN-NAME: vmail
